<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
    PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
    "https://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.korit.springboot.mapper.StudyMapper">
    <insert id="insert">
        insert into study_tb
        values (default, #{name}, #{age} )
    </insert>
    <select id="findAllName">
        select
            name
        from
            study_tb
<!--        값이 여러개면 알아서 List로 묶음-->
    </select>
</mapper>